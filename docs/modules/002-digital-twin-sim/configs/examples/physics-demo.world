<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="physics_demo_world">
    <!-- Physics engine configuration optimized for humanoid robot simulation -->
    <physics name="ode_physics" type="ode">
      <gravity>0 0 -9.8</gravity>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
      <real_time_update_rate>1000.0</real_time_update_rate>
      <ode>
        <solver>
          <type>quick</type>
          <iters>10</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>1e-5</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
          <contact_surface_layer>0.001</contact_surface_layer>
        </constraints>
      </ode>
    </physics>

    <!-- Ground plane with realistic friction properties -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Lighting -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Simple humanoid robot model -->
    <model name="simple_humanoid">
      <pose>0 0 0.8 0 0 0</pose>

      <!-- Torso -->
      <link name="torso">
        <pose>0 0 0.7 0 0 0</pose>
        <collision name="torso_collision">
          <geometry>
            <box>
              <size>0.3 0.2 0.6</size>
            </box>
          </geometry>
        </collision>
        <visual name="torso_visual">
          <geometry>
            <box>
              <size>0.3 0.2 0.6</size>
            </box>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>5.0</mass>
          <inertia>
            <ixx>0.2</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.2</iyy>
            <iyz>0</iyz>
            <izz>0.2</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Head -->
      <link name="head">
        <pose>0 0 1.1 0 0 0</pose>
        <collision name="head_collision">
          <geometry>
            <sphere>
              <radius>0.1</radius>
            </sphere>
          </geometry>
        </collision>
        <visual name="head_visual">
          <geometry>
            <sphere>
              <radius>0.1</radius>
            </sphere>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.004</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.004</iyy>
            <iyz>0</iyz>
            <izz>0.004</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Left upper arm -->
      <link name="left_upper_arm">
        <pose>-0.2 0 0.7 0 0 0</pose>
        <collision name="left_upper_arm_collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.3</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="left_upper_arm_visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.3</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.005</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.005</iyy>
            <iyz>0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Left lower arm -->
      <link name="left_lower_arm">
        <pose>-0.35 0 0.7 0 0 0</pose>
        <collision name="left_lower_arm_collision">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.25</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="left_lower_arm_visual">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.25</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.3</mass>
          <inertia>
            <ixx>0.003</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.003</iyy>
            <iyz>0</iyz>
            <izz>0.0008</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Right upper arm -->
      <link name="right_upper_arm">
        <pose>0.2 0 0.7 0 0 0</pose>
        <collision name="right_upper_arm_collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.3</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="right_upper_arm_visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.3</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.005</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.005</iyy>
            <iyz>0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Right lower arm -->
      <link name="right_lower_arm">
        <pose>0.35 0 0.7 0 0 0</pose>
        <collision name="right_lower_arm_collision">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.25</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="right_lower_arm_visual">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.25</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.3</mass>
          <inertia>
            <ixx>0.003</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.003</iyy>
            <iyz>0</iyz>
            <izz>0.0008</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Left upper leg -->
      <link name="left_upper_leg">
        <pose>-0.1 0 0.3 0 0 0</pose>
        <collision name="left_upper_leg_collision">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="left_upper_leg_visual">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.015</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.015</iyy>
            <iyz>0</iyz>
            <izz>0.003</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Left lower leg -->
      <link name="left_lower_leg">
        <pose>-0.1 0 -0.1 0 0 0</pose>
        <collision name="left_lower_leg_collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="left_lower_leg_visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.8</mass>
          <inertia>
            <ixx>0.01</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.01</iyy>
            <iyz>0</iyz>
            <izz>0.002</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Right upper leg -->
      <link name="right_upper_leg">
        <pose>0.1 0 0.3 0 0 0</pose>
        <collision name="right_upper_leg_collision">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="right_upper_leg_visual">
          <geometry>
            <cylinder>
              <radius>0.06</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.015</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.015</iyy>
            <iyz>0</iyz>
            <izz>0.003</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Right lower leg -->
      <link name="right_lower_leg">
        <pose>0.1 0 -0.1 0 0 0</pose>
        <collision name="right_lower_leg_collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="right_lower_leg_visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.4</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.6 0.6 0.6 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.8</mass>
          <inertia>
            <ixx>0.01</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.01</iyy>
            <iyz>0</iyz>
            <izz>0.002</izz>
          </inertia>
        </inertial>
      </link>

      <!-- Joints connecting the links -->
      <joint name="torso_head" type="revolute">
        <parent>torso</parent>
        <child>head</child>
        <axis xyz="0 1 0">
          <limit lower="-0.5" upper="0.5" effort="10" velocity="1"/>
          <dynamics damping="0.1" friction="0.1"/>
        </axis>
        <pose>0 0 0.4 0 0 0</pose>
      </joint>

      <joint name="torso_left_shoulder" type="revolute">
        <parent>torso</parent>
        <child>left_upper_arm</child>
        <axis xyz="0 1 0">
          <limit lower="-1.57" upper="1.57" effort="20" velocity="2"/>
          <dynamics damping="0.2" friction="0.15"/>
        </axis>
        <pose>-0.15 0 0.3 0 0 0</pose>
      </joint>

      <joint name="left_shoulder_elbow" type="revolute">
        <parent>left_upper_arm</parent>
        <child>left_lower_arm</child>
        <axis xyz="0 0 1">
          <limit lower="-1.57" upper="1.57" effort="15" velocity="2"/>
          <dynamics damping="0.1" friction="0.1"/>
        </axis>
        <pose>-0.15 0 0 0 0 0</pose>
      </joint>

      <joint name="torso_right_shoulder" type="revolute">
        <parent>torso</parent>
        <child>right_upper_arm</child>
        <axis xyz="0 1 0">
          <limit lower="-1.57" upper="1.57" effort="20" velocity="2"/>
          <dynamics damping="0.2" friction="0.15"/>
        </axis>
        <pose>0.15 0 0.3 0 0 0</pose>
      </joint>

      <joint name="right_shoulder_elbow" type="revolute">
        <parent>right_upper_arm</parent>
        <child>right_lower_arm</child>
        <axis xyz="0 0 1">
          <limit lower="-1.57" upper="1.57" effort="15" velocity="2"/>
          <dynamics damping="0.1" friction="0.1"/>
        </axis>
        <pose>0.15 0 0 0 0 0</pose>
      </joint>

      <joint name="torso_left_hip" type="revolute">
        <parent>torso</parent>
        <child>left_upper_leg</child>
        <axis xyz="0 1 0">
          <limit lower="-1.57" upper="0.5" effort="30" velocity="1.5"/>
          <dynamics damping="0.3" friction="0.2"/>
        </axis>
        <pose>-0.1 0 0.1 0 0 0</pose>
      </joint>

      <joint name="left_hip_knee" type="revolute">
        <parent>left_upper_leg</parent>
        <child>left_lower_leg</child>
        <axis xyz="0 0 1">
          <limit lower="0" upper="1.57" effort="25" velocity="1.5"/>
          <dynamics damping="0.2" friction="0.15"/>
        </axis>
        <pose>0 0 -0.2 0 0 0</pose>
      </joint>

      <joint name="torso_right_hip" type="revolute">
        <parent>torso</parent>
        <child>right_upper_leg</child>
        <axis xyz="0 1 0">
          <limit lower="-1.57" upper="0.5" effort="30" velocity="1.5"/>
          <dynamics damping="0.3" friction="0.2"/>
        </axis>
        <pose>0.1 0 0.1 0 0 0</pose>
      </joint>

      <joint name="right_hip_knee" type="revolute">
        <parent>right_upper_leg</parent>
        <child>right_lower_leg</child>
        <axis xyz="0 0 1">
          <limit lower="0" upper="1.57" effort="25" velocity="1.5"/>
          <dynamics damping="0.2" friction="0.15"/>
        </axis>
        <pose>0 0 -0.2 0 0 0</pose>
      </joint>
    </model>

    <!-- Example objects for interaction -->
    <model name="physics_box">
      <pose>1.5 0 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>0.2 0.2 0.2</size>
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.8</mu>
                <mu2>0.8</mu2>
              </ode>
            </friction>
            <bounce>
              <restitution_coefficient>0.1</restitution_coefficient>
            </bounce>
            <contact>
              <ode>
                <soft_cfm>0.000001</soft_cfm>
                <soft_erp>0.2</soft_erp>
                <kp>1000000000000.0</kp>
                <kd>1.0</kd>
              </ode>
            </contact>
          </surface>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.2 0.2 0.2</size>
            </box>
          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.0033</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0033</iyy>
            <iyz>0</iyz>
            <izz>0.0033</izz>
          </inertia>
        </inertial>
      </link>
    </model>

    <model name="physics_sphere">
      <pose>-1.5 0.5 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <sphere>
              <radius>0.1</radius>
            </sphere>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.5</mu>
                <mu2>0.5</mu2>
              </ode>
            </friction>
            <bounce>
              <restitution_coefficient>0.3</restitution_coefficient>
            </bounce>
          </surface>
        </collision>
        <visual name="visual">
          <geometry>
            <sphere>
              <radius>0.1</radius>
            </sphere>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>0.3</mass>
          <inertia>
            <ixx>0.0012</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0012</iyy>
            <iyz>0</iyz>
            <izz>0.0012</izz>
          </inertia>
        </inertial>
      </link>
    </model>

    <!-- Ramp for demonstrating physics interactions -->
    <model name="ramp">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <mesh>
              <uri>file://ramp.dae</uri>
            </mesh>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <mesh>
              <uri>file://ramp.dae</uri>
            </mesh>
          </geometry>
        </visual>
        <inertial>
          <mass>10.0</mass>
          <inertia>
            <ixx>1.0</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1.0</iyy>
            <iyz>0</iyz>
            <izz>1.0</izz>
          </inertia>
        </inertial>
      </link>
      <pose>2 0 0 0 0 0</pose>
    </model>

    <!-- A simple box ramp as an alternative -->
    <model name="simple_ramp">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>1.0 2.0 0.2</size>
            </box>
          </geometry>
          <pose>2 0 0.1 0 0.3 0</pose>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>1.0 2.0 0.2</size>
            </box>
          </geometry>
          <pose>2 0 0.1 0 0.3 0</pose>
          <material>
            <ambient>0.6 0.4 0.2 1</ambient>
            <diffuse>0.8 0.6 0.4 1</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>10.0</mass>
          <inertia>
            <ixx>1.0</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1.0</iyy>
            <iyz>0</iyz>
            <izz>1.0</izz>
          </inertia>
        </inertial>
      </link>
    </model>
  </world>
</sdf>